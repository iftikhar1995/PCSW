            function checkInputData() {
                clearAllErrors();

                //------------------------------1---------------------------------------
                var numBoardBCT = $("#numBoardBCT").val();
                if (!(/^\d+$/.test(numBoardBCT)) || Number(numBoardBCT) < 0) {
                    numBoardBCTErr.html("<span>Please Enter a Valid Number</span>");
