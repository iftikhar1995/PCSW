                vacantPositionsCommitteeErr.text("");
                vacantPositionsTaskForceErr.text("");

                numMemberTrainingsErr.text("");

            }


            function writeXlsFile( ) {
                d = {
                    focalPerson: sessionStorage.focalPerson, contactNumber: sessionStorage.contactNumber,
                    gazettedMale: sessionStorage.gazettedMale, gazettedFemale: sessionStorage.gazettedFemale,
                    noneGazettedMale: sessionStorage.noneGazettedMale, noneGazettedFemale: sessionStorage.noneGazettedFemale,
                    contractMale: sessionStorage.contractMale, contractFemale: sessionStorage.contractFemale,
                    washroomsFemale: sessionStorage.washroomsFemale, prayerRoomsFemale: sessionStorage.prayerRoomsFemale,
                    numAgeRelexation3: sessionStorage.numAgeRelexation3, numMaternityLeave: sessionStorage.numMaternityLeave,
                    numPaternityLeave: sessionStorage.numPaternityLeave, numSelectionContractualCommittee: sessionStorage.numSelectionContractualCommittee,
                    numComplaintsReceived: sessionStorage.numComplaintsReceived, numActionsTaken: sessionStorage.numActionsTaken,
                    GMC: sessionStorage.GMC, COCPunjabProtection: sessionStorage.COCPunjabProtection,
                    workplaceHarassmentCommittees: sessionStorage.workplaceHarassmentCommittees, numBoardBCT: sessionStorage.numBoardBCT,
                    numCommitteeBCT: sessionStorage.numCommitteeBCT, numTaskForceBCT: sessionStorage.numTaskForceBCT,
                    numMaleBoard: sessionStorage.numMaleBoard, numMaleCommittee: sessionStorage.numMaleCommittee,
                    numMaleTaskForce: sessionStorage.numMaleTaskForce, numFemaleBoard: sessionStorage.numFemaleBoard,
                    numFemaleCommittee: sessionStorage.numFemaleCommittee, numFemaleTaskForce: sessionStorage.numFemaleTaskForce,
                    tenureOfBoard: sessionStorage.tenureOfBoard, tenureOfCommittee: sessionStorage.tenureOfCommittee,
                    tenureOfTaskForce: sessionStorage.tenureOfTaskForce, vacantPositionsBoard: sessionStorage.vacantPositionsBoard,
                    vacantPositionsCommittee: sessionStorage.vacantPositionsCommittee, vacantPositionsTaskForce: sessionStorage.vacantPositionsTaskForce,
                    numMemberTrainings: sessionStorage.numMemberTrainings, year: sessionStorage.year
                };


                var request =  $.ajax({
                                    url: '@Url.Action("ExportToXLS", "GIMS")',
                                    type: "POST",
                                    data: d,
                                    dataType: "json",
                                    success: function (data) {
                                        sessionStorage.FileId = data.fileId;
                                        window.location.href = '@Url.Action("DownloadView", "GIMS")';
                                    },
                                    error: function () {
                                        window.location.href = '@Url.Action("ErrorView", "GIMS")';
                                    }
                });


            }


        });


